<!--Please note the -->
<mujoco model="sirius_wheel">
    <compiler angle="radian" meshdir="assets" autolimits="true" eulerseq="ZYX"/>
    <!--    <option cone="elliptic" impratio="100" gravity="0 0 0"/>-->
    <option cone="elliptic" impratio="100" integrator="implicitfast" timestep="0.001"/>

    <visual>
        <global ellipsoidinertia="true"/>
    </visual>
    <!--    mujoco default properties-->
    <default>
        <default class="sirius_wheel">
            <geom friction="0.6" margin="0.00001" condim="1"/>
            <!--            armature: inertia of rotors; joint global axis 0.0000097913844*gr^2-->
            <joint axis="0 1 0" damping="0.01" armature="0.01" frictionloss="0.01"/>
            <!--            <position kp="200" forcerange="-45 45"/>-->
            <default class="abad">
                <!--                joint range-->
                <joint pos="0 0 0" axis="1 0 0" range="-1.57 1.57"/>
                <!--                control position -->
                <!--                <position ctrlrange="-0.863 0.863"/>-->
                <motor ctrlrange="-40 40" forcerange="-40 40"/>
            </default>
            <default class="hip">
                <joint range="-3.1415926 3.1415926"/>
                <motor ctrlrange="-40 40" forcerange="-40 40"/>
            </default>

            <default class="knee">
                <joint range="-2.9 2.9"/>
                <!--                output torque range, knees with larger gear ratio-->
                <motor forcerange="-80 80" ctrlrange="-80 80"/>
            </default>

            <default class="left_wheel">
                <joint range="-10000000 10000000"/>
                <motor ctrlrange="-40 40" forcerange="-40 40"/>
            </default>

            <default class="right_wheel">
                <joint axis="0 -1 0" range="-10000000 10000000"/>
                <motor ctrlrange="-40 40" forcerange="-40 40"/>
            </default>
            <!--            visual mesh for sensors and markers-->
            <default class="visual">
                <geom type="mesh" contype="0" conaffinity="0" group="2"/>
            </default>
            <!--            build collision model-->
            <default class="collision">
                <geom group="3" type="capsule"/>
                <default class="foot">
                    <geom type="sphere" size="0.024" priority="1" condim="6"
                          friction="0.5 0.02 0.01"
                          pos="0 0 -0.191"/>
                </default>
                <!--                body is based on local frame-->
                <default class="thigh_fl1">
                    <geom size="0.048 0.05685" pos="0 0.05735 0" quat="1 1 0 0" type="cylinder"/>
                </default>
                <default class="thigh_fl2">
                    <geom size="0.025 0.08295" pos="0 0.1282 -0.10795" quat="1 0 0 0" type="capsule"/>
                </default>


                <default class="thigh_fr1">
                    <geom size="0.048 0.05685" pos="0 -0.05735 0" quat="1 1 0 0" type="cylinder"/>
                </default>
                <default class="thigh_fr2">
                    <geom size="0.025 0.08295" pos="0 -0.1282 -0.10795" quat="1 0 0 0" type="capsule"/>
                </default>


                <default class="thigh_rl1">
                    <geom size="0.048 0.05685" pos="0 0.05735 0" quat="1 1 0 0" type="cylinder"/>
                </default>
                <default class="thigh_rl2">
                    <geom size="0.025 0.08295" pos="0 0.1282 -0.10795" quat="1 0 0 0" type="capsule"/>
                </default>

                <default class="thigh_rr1">
                    <geom size="0.048 0.05685" pos="0 -0.05735 0" quat="1 1 0 0" type="cylinder"/>
                </default>
                <default class="thigh_rr2">
                    <geom size="0.025 0.08295" pos="0 -0.1282 -0.10795" quat="1 0 0 0" type="capsule"/>
                </default>

                <default class="calf">
                    <geom size="0.01 0.0855" pos="0 0 -0.0955" quat="1 0 0 0" type="capsule"/>
                </default>
            </default>
        </default>
    </default>

