<mujoco model="go1">
    <compiler angle="radian" meshdir="assets" autolimits="true" eulerseq="zyx"/>

    <option cone="elliptic" impratio="100" integrator="implicitfast" timestep="0.001" />
    <option iterations="50" solver="PGS" gravity="0 0 -9.81"/>
    <default>
